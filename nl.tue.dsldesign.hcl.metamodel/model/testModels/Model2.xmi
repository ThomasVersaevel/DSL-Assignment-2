<?xml version="1.0" encoding="UTF-8"?>
<hcl:Model xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:hcl="http://www.example.org/hcl"
    xsi:schemaLocation="http://www.example.org/hcl ../hcl.ecore">
  <computer
      label="my_computer">
    <component
        xsi:type="hcl:ProcessingUnit"
        label="my_CPU"
        cores="4"
        speed="2.0">
      <l1 size="64"
          processingunit="//@computer.0/@component.0"/>
    </component>
    <component
        xsi:type="hcl:StorageUnit"
        label="storage1">
      <storagedevice
          size="500"/>
    </component>
    <component
        xsi:type="hcl:DisplayUnit"
        label="my_display"
        diagonal="30.0"
        type="5K"/>
  </computer>
</hcl:Model>
