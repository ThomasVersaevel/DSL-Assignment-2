<?xml version="1.0" encoding="UTF-8"?>
<hcl:Model xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:hcl="http://www.example.org/hcl"
    xsi:schemaLocation="http://www.example.org/hcl ../hcl.ecore">
  <computer
      label="my_computer">
    <component
        xsi:type="hcl:DisplayUnit"
        label="my_display"
        diagonal="20.0"/>
    <component
        xsi:type="hcl:ProcessingUnit"
        label="my_cpu"
        cores="4"
        speed="4.6">
      <l1 size="64"
          processingunit="//@computer.0/@component.1"/>
    </component>
    <component
        xsi:type="hcl:ProcessingUnit"
        label="my_cpu2"
        cores="6"
        speed="4.6"/>
    <component
        xsi:type="hcl:DisplayUnit"
        label="my_disp2"
        diagonal="20.0"
        type="FullHD"/>
  </computer>
  <computer
      label="comp2">
    <component
        xsi:type="hcl:DisplayUnit"
        label="disp_3"
        diagonal="20.0"/>
  </computer>
</hcl:Model>
